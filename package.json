{
  "name": "nodeCli",
  "version": "1.0.0",
  "description": "使用 egg.js 搭建一套开箱即用的web服务",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.26.0",
    "egg-mysql": "^3.0.0",
    "egg-scripts": "^2.13.0",
    "egg-sequelize": "^5.2.1",
    "egg-socket.io": "^4.1.6",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.1",
    "egg-ci": "^1.13.1",
    "egg-mock": "^4.0.0",
    "eslint": "^6.8.0",
    "eslint-config-egg": "^8.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-nodeCli",
    "stop": "egg-scripts stop --title=egg-server-nodeCli",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "adminV",
  "license": "MIT"
}
